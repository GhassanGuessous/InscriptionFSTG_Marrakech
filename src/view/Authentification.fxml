      <Button layoutX="259.0" layoutY="282.0" mnemonicParsing="false" onAction="#sAuthentifier" prefHeight="32.0" prefWidth="82.0" text="OK" />
      <Button layoutX="36.0" layoutY="347.0" mnemonicParsing="false" onAction="#retour" text="Accueil">
         <font>
            <Font name="Calibri" size="12.0" />
         </font>
      </Button>
